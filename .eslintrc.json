{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-explicit-any": [
      "error",
      {
        "ignoreRestArgs": true,  // Это позволит использовать any для параметров catch
        "fixToUnknown": true    // По умолчанию лучше использовать unknown вместо any
      }
    ]
  }
}
